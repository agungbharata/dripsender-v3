---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import GoogleAnalytic from "../components/GoogleAnalytic.astro";

export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<html lang="en">
  <head>
     <!-- GoogleAnalytic Tag -->
    <GoogleAnalytic/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>Dripsender - Membantu Ledakkan Omset Bisnis via Whatsapp Marketing</title>
    <meta name="description" content="Tools WhatsApp Marketing Nomor 1 di Indonesia" />
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap" rel="stylesheet" />
    <script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.13.0/dist/cdn.min.js"></script>

    


    <meta property="og:url" content={Astro.url.href.replace("dripsender.id","www.dripsender.id")} />
    <meta property="og:type" content="article" />
    <meta property="og:title" content="Membantu Ledakkan Omset Bisnis via Whatsapp Marketing" />
    <meta property="og:description" content="Pelajari selengkapnya disini" />

    <meta property="og:image" content={Astro.url.origin.replace("dripsender.id","www.dripsender.id")+"/assets/images/Membantu-Ledakkan.jpg"} />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="628" />

    <meta name="twitter:title" content="Membantu Ledakkan Omset Bisnis via Whatsapp Marketing" />
    <meta name="twitter:description" content="Pelajari selengkapnya disini" />
    <meta name="twitter:image" content={Astro.url.origin.replace("dripsender.id","www.dripsender.id")+"/assets/images/Membantu-Ledakkan.jpg"} />
     <meta name="twitter:card" content="summary_large_image" />
  </head>
  <body class="overflow-hidden overflow-y-auto bg-auto">
    <Header />
    <slot />
    <Footer />
  </body>
</html>
